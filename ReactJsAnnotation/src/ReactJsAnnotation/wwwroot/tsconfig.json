{
	"compilerOptions": {
		"target": "es5",
		"module": "amd",
		"moduleResolution": "node",
		"isolatedModules": false,
		"jsx": "react",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"declaration": false,
		"noImplicitAny": false,
		"removeComments": true,
		"noLib": false,
		"preserveConstEnums": true,
		"suppressImplicitAnyIndexErrors": true
	},
	"filesGlob": [
		"**/*.ts",
		"**/*.tsx",
		"!node_modules/**"
	],
	"files": [
		//"js/register/register.tsx"
	],
	"exclude": [ ]
}
